      <value>#{newaFighterManager}</value>
    </managed-property>
    <managed-property>
      <property-name>configManager</property-name>
      <value>#{configManager}</value>
    </managed-property>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>tatamiNewaclassOverview</managed-bean-name>
    <managed-bean-class>de.jjw.webapp.action.off.newa.NewaclassOverviewAction</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>newaclassManager</property-name>
      <value>#{newaclassManager}</value>

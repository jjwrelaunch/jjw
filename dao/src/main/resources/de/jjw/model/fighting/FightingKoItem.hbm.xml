<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="de.jjw.model.fighting.FightingKoItem"
            table="fighting_ko_poolentries"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="increment">

    </generator>

    </id>

        <property
            name="fightingclassId"
                    column="poolId"
        >

        </property>

        <many-to-one
            name="fightingclass"
                    class="de.jjw.model.fighting.Fightingclass"
                    column="poolId"
                insert="false"
                update="false"
        >

        </many-to-one>

        <many-to-one
            name="fighter"
                    class="de.jjw.model.fighting.Fighter"
                    column="fighterId"
                not-null="false"
                insert="false"
                update="false"
        >

        </many-to-one>

        <property
            name="fighterId"
                    column="fighterId"
        >

        </property>

        <property
            name="numberInPool"
                    column="numberInPool"
        >

        </property>

        <property
            name="poolPart"
                    column="poolPart"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                not-null="true"
        >

        </property>

        <property
            name="createId"
                    column="createId"
        >

        </property>

        <property
            name="modificationDate"
                    column="modificationDate"
                not-null="true"
        >

        </property>

        <property
            name="modificationId"
                    column="modificationId"
                not-null="true"
        >

        </property>

    </class>    

</hibernate-mapping>


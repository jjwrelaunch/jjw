<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="de.jjw.model.fighting.Fighter"
            table="fighter"
    >

    <id
        name="id"
            column="id"
            unsaved-value="null"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="increment">

    </generator>

    </id>

        <property
            name="firstname"
                    column="firstname"
                length="30"
        >

        </property>

        <property
            name="name"
                    column="name"
                length="30"
        >

        </property>

        <many-to-one
            name="age"
                    class="de.jjw.model.admin.Age"
                    column="age"
                not-null="true"
                insert="false"
                update="false"
        >

        </many-to-one>

        <many-to-one
            name="team"
                    class="de.jjw.model.Team"
                    column="team"
                not-null="true"
                insert="false"
                update="false"
        >

        </many-to-one>

        <property
            name="place"
                    column="place"
        >

        </property>

        <property
            name="ready"
                    column="ready"
                length="1"
        >

        </property>

        <property
            name="sex"
                    column="sex"
                not-null="true"
        >

        </property>

        <property
            name="weight"
                    column="weight"
        >

        </property>

        <many-to-one
            name="fightingclass"
                    class="de.jjw.model.fighting.Fightingclass"
                    column="fightingclass"
                insert="false"
                update="false"
        >

        </many-to-one>

        <property
            name="delete"
                    type="yes_no"
                    column="delete_flag"
                length="1"
        >

        </property>

        <property
            name="ageId"
                    column="age"
        >

        </property>

        <property
            name="teamId"
                    column="team"
        >

        </property>

        <property
            name="fightingclassId"
                    column="fightingclass"
        >

        </property>

        <property
            name="originalFightingclassId"
                    column="originalFightingclass"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                not-null="true"
        >

        </property>

        <property
            name="createId"
                    column="createId"
        >

        </property>

        <property
            name="modificationDate"
                    column="modificationDate"
                not-null="true"
        >

        </property>

        <property
            name="modificationId"
                    column="modificationId"
                not-null="true"
        >

        </property>
        
        <property
            name="yearOfBirth"
                    column="yearOfBirth"
        >

        </property>
        
         <property
            name="markForExport"
                    type="yes_no"
                    column="markForExport"        
          >
          </property>
    </class>    

</hibernate-mapping>


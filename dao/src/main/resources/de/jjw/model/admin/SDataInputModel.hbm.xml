<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="de.jjw.model.admin.SDataInputModel"
            table="sdataInput"
    >

    <id        name="id"            column="id"            unsaved-value="null"    >
    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="increment">

    </generator>

    </id>

        <property name="firstname"  column="firstname" length="30" >
        </property>

        <property name="name"   column="name" length="30" >
        </property>

        <property  name="sex"  column="sex"  not-null="true"  >
        </property>

        <property name="weight" column="weight"    >
        </property>

        <property  name="teamId"  column="teamId"   >
        </property>

        <property name="createDate"   column="createDate"  not-null="true"        >
        </property>

        <property  name="createId" column="createId"      >
        </property>

        <property     name="modificationDate"   column="modificationDate"   not-null="true"        >
        </property>

        <property    name="modificationId"   column="modificationId"    not-null="true"       >
        </property>
        
        <property  name="dateOfBirth"  column="dateOfBirth"   >
        </property>
        <property  name="discepline" column="discepline"  >
        </property>
        <property  name="duoTeamName"  column="duoTeamName"  >
        </property>
        
         <property    name="teamname"    column="teamname"  >
          </property>
          
          <property    name="countryCode"    column="countryCode"  >
          </property>
    </class>    

</hibernate-mapping>


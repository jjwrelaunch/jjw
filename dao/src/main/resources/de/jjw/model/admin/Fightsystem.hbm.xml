<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="de.jjw.model.admin.Fightsystem"
            table="fightsystem"
    >

    <id
        name="id"
            column="id"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="increment">

    </generator>

    </id>

        <property
            name="fightsystemType"
                    column="system"
        >

        </property>

        <property
            name="maxParticipant"
                    column="maxFighter"
        >

        </property>

        <property
            name="minParticipant"
                    column="minFighter"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                not-null="true"
        >

        </property>

        <property
            name="createId"
                    column="createId"
        >

        </property>

        <property
            name="modificationDate"
                    column="modificationDate"
                not-null="true"
        >

        </property>

        <property
            name="modificationId"
                    column="modificationId"
                not-null="true"
        >

        </property>

    </class>    

</hibernate-mapping>


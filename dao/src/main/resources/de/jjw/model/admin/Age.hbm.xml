                    column="estimatedTimeNewa"
        >

        </property>

        <property
            name="fightingTimeNewa"
                    column="fightingTimeNewa"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                not-null="true"
        >

        </property>

        <property
            name="createId"
                    column="createId"
        >

        </property>

        <property
            name="modificationDate"
                    column="modificationDate"
                not-null="true"
        >

        </property>

        <property
            name="modificationId"
                    column="modificationId"
                not-null="true"
        >

        </property>

    </class>    

</hibernate-mapping>

